    {
      "cell_type": "code",
      "source": [
        "# Define custom functions\n",
        "def bq_query_to_df(query):\n",
        "    query_job = client.query(query)\n",
        "    return query_job.to_dataframe()\n",
        "\n",
        "\n",
        "query1='''\n",
        "SELECT * FROM `encoded-metrics-398203.datos_icfes.icefes_2019` LIMIT 1000\n",
        "'''\n",
        "\n",
        "df=bq_query_to_df(query1)"
